  },
  "keywords": [
    "editor",
    "tistory"
  ],
  "author": {
    "name": "Joo",
    "email": "joo@joostory.net",
    "url": "http://joostory.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joostory/tistory-editor/issues"
  },
  "homepage": "https://github.com/joostory/tistory-editor#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",

{
  "name": "tistory-editor",
  "productName": "TistoryEditor",
  "version": "0.4.0",
  "description": "Editor for tistory",
  "main": "src/main/main.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "watch": "NODE_ENV=development webpack --watch",
    "app": "NODE_ENV=development electron .",
    "start": "concurrently \"npm run watch\" \"npm run app\"",
    "test": "jest -u",
    "dist": "build --linux --win --mac zip --x64",
